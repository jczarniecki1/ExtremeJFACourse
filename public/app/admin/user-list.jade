.container
    table.table
        tr
            th Name
            th Username
            th Roles
            th Actions
        tr(ng-repeat="user in users")
            td {{ user.firstName + ' ' + user.lastName }}
            td {{ user.username }}
            td {{ user.roles.join(', ') }}
            td
                a.btn.btn-danger(ng-click="delete(user._id)") x