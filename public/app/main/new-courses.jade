.panel.panel-primary
    .panel-heading.text-center New Courses
    .panel-body
        .row(ng-repeat="course in courses | orderBy:'publishDate':true | filter:{ published:true } | limitTo:10")
            .col-md-3 {{ course.publishDate | date:'MMMM d' }}
            .col-md-9
                a(href="/courses/{{ course._id }}") {{ course.title }}